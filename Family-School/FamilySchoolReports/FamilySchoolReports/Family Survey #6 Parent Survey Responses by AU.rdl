        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DisabilityMatch">
      <Query>
        <DataSourceName>DataSourceSSRS</DataSourceName>
        <CommandText>select ID, Name
from (values ('All', 'All'), ('Yes', 'Yes'), ('No', 'No'), ('Blank', 'Blank')) t (ID, Name)</CommandText>
