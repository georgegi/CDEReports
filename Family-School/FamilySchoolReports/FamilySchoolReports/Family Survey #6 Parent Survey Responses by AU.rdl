  <DataSources>
    <DataSource Name="DataSourceSSRS">
      <DataSourceReference>SSRS datasource</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>f05833c6-026d-4bdb-a959-73ae3af638d7</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSourceSSRS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@userID">
            <Value>=Parameters!userID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@language">
            <Value>=Parameters!language.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@disabMatch">
            <Value>=Parameters!disabMatch.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@rosterYearID">
            <Value>=Parameters!rosterYearID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>; with xCTE as (
	select 
		AdminUnit 
		, Question = right(' '+cast(m.QuestionSequence as varchar(2)), 2)+' - '+m.Question
		, m.LikertScore 
		, Weight = m.ResponseWeight
		, m.WeightedScore 
	from x_DATATEAM.FamilySurveyMaster m
	where 
		OrgUnitID in (select OrgUnitID from UserProfileOrgUnit where UserProfileID = @userID) AND
		(@language = 'All' or cast(Language as varchar(10)) = @language) AND
		(@disabMatch = 'All' or cast(DisabilityMatch as varchar(10)) = @disabMatch) AND
		m.RosterYearID = @rosterYearID
)

select 
	AdminUnit
	, Question
	, Score = sum(Score)
	, Weight
	, WeightedScore = sum(WeightedScore)
from (
select 
	AdminUnit = case when grouping(x.AdminUnit) = 0 then x.AdminUnit else 'Total' end
	, Question = case when grouping(x.Question) = 0 then x.Question else 'Total' end
	, Score = sum(x.likertscore)
	, Weight = case when grouping(x.Question) = 0 then x.Weight else NULL end -- case when grouping(x.Weight) = 0 then x.Weight else NULL end
	, WeightedScore = sum(x.weightedscore)
from xCTE x
group by rollup (AdminUnit, Question), Weight
) y
group by AdminUnit, Question, Weight
order by AdminUnit, Question</CommandText>
      </Query>
      <Fields>
        <Field Name="AdminUnit">
          <DataField>AdminUnit</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Question">
          <DataField>Question</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Score">
          <DataField>Score</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Weight">
          <DataField>Weight</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="WeightedScore">
          <DataField>WeightedScore</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="RosterYears">
      <Query>
        <DataSourceName>DataSourceSSRS</DataSourceName>
        <CommandText>select distinct ID = RosterYearID, RosterYear
from x_DATATEAM.FamilySurveyMaster
order by 2</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="RosterYear">
          <DataField>RosterYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Language">
      <Query>
        <DataSourceName>DataSourceSSRS</DataSourceName>
        <CommandText>select ID, Name 
from (Values ('All', 'All'), ('English', 'English'), ('Spanish', 'Spanish'), ('Blank', 'Blank')) t (ID, Name)</CommandText>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
