  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SSRS_Compliance_Reports_Datasource</DataSourceName>
        <CommandText>
; with recCTE as (
select
	 p.AU
	, p.SubmissionModeID
	, IsExcluded = case when sel.ExcludedDate is null then 0 else 1 end
	, AttributeID = a.ID
	, Attribute = a.Name
	, AttributeSequence = a.SequenceNumber
	, Value = convert(varchar(max), v.Value)
	, InstanceID = rf.ID
	, RecordID = r.ID
	, RecordSelectionSequence = sel.SequenceNumber
	, SASID = convert(varchar(20), SAS.Value)
from x_DATATEAM.RawDataParameters p -- select * from x_DATATEAM.RawDataParameters 
join dbo.MonRecordSelection sel on p.RecordSetID = sel.RecordSetID
left join dbo.MonSubmissionRecord subrec on sel.ID = subrec.RecordSelectionID and p.SubmissionsID = subrec.SubmissionsID
left join dbo.MonSubmissionRecordForm rf on subrec.ID = rf.SubmissionRecordID
join dbo.MonRecord r on sel.RecordID = r.ID
join dbo.MonRecordAttributeValue v on r.ID = v.RecordID
join dbo.MonRecordAttribute a on v.AttributeID = a.ID
join dbo.MonRecordAttributeValue SAS on r.ID = SAS.RecordID and SAS.AttributeID = '817E95B3-A6D5-4561-8652-6E27FA3C7A5F' -- SASID
where p.RecordSetSampledFrom = 'RDA_Ind8_Parent_Survey_Samples_01_16_2015' -- 58 districts
and p.RecordSetId = '4F2044F2-0FFF-4883-A60E-349490327BD6'
-- and a.ID = '817E95B3-A6D5-4561-8652-6E27FA3C7A5F' -- SASID
) 

--select Type = 'MonRecord', r.AU, r.IsExcluded, r.AttributeID, r.Attribute, r.AttributeSequence, r.Value, r.InstanceID, r.RecordID, r.RecordSelectionSequence, SASID = r2.Value
--from recCTE r
--join recCTE r2 on r.RecordID = r2.RecordID and r2.AttributeID = '817E95B3-A6D5-4561-8652-6E27FA3C7A5F' 

select *
from (
select Type = 'MonRecord', r.AU, r.IsExcluded, r.AttributeID, r.Attribute, r.AttributeSequence, r.Value, r.InstanceID, r.RecordID, r.RecordSelectionSequence, r.SASID
from recCTE r
	union all
select 'MonForm', r.AU, r.IsExcluded, a.AttributeID, a.Attribute, a.AttributeSequence, f.Value, r.InstanceID, r.RecordID, r.RecordSelectionSequence, r.SASID
from recCTE r
join x_DATATEAM.MonForm_Attribute a on r.SubmissionModeID = a.SubmissionModeID
left join x_datateam.monformlet_key_attribute_value f on isnull(r.InstanceID,'00000000-0000-0000-0000-000000000000') = f.InstanceID 
	and a.AttributeID = f.AttributeID -- there are some NULLs from r
where a.SubmissionModeID = '1A4C0468-C2F1-442A-85DA-F9F13E6CE435'
and r.AttributeID = 'BA4E2A1C-9A70-4D87-B61B-87132CA9BF56' -- limit to one r record to avoid cartesian
) x
--where x.InstanceID = 'C00BF05C-4828-4DBC-8F94-C86028267EC6'
order by x.RecordSelectionSequence, x.type desc, x.AttributeSequence


</CommandText>
