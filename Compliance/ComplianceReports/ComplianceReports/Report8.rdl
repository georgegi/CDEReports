          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReportLabel">
          <DataField>ReportLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InputItemLabel">
          <DataField>InputItemLabel</DataField>
