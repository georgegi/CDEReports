        </Field>
        <Field Name="PerItemPct">
          <DataField>PerItemPct</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SortBit">
          <DataField>SortBit</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
